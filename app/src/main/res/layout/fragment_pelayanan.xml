<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.PelayananFr">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/laporkan"
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_margin="10dp"
            android:src="@drawable/tidakpuas"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:paddingBottom="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/laporkan"
            tools:ignore="SpeakableTextPresentCheck">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/plSPKT"
                    android:layout_width="match_parent"
                    android:layout_height="175dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/yanspkt" />

                <ImageView
                    android:id="@+id/plSIM"
                    android:layout_width="match_parent"
                    android:layout_height="175dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/yansim" />

                <ImageView
                    android:id="@+id/plSKCK"
                    android:layout_width="match_parent"
                    android:layout_height="175dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/yanskck" />

                <ImageView
                    android:id="@+id/plIDEN"
                    android:layout_width="match_parent"
                    android:layout_height="175dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/yaniden" />

                <ImageView
                    android:id="@+id/plVAKSIN"
                    android:layout_width="match_parent"
                    android:layout_height="175dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/yanvaksin" />
            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>